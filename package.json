{
  "name": "@michaelvanlaar/n8n-nodes-defuddle",
  "version": "0.2.5",
  "description": "n8n node to extract main content from webpages using Defuddle library",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "defuddle",
    "content-extraction",
    "web-scraping",
    "readability"
  ],
  "license": "MIT",
  "homepage": "https://github.com/MichaelvanLaar/n8n-nodes-defuddle",
  "author": {
    "name": "Michael van Laar",
    "email": "michael@van-laar.de"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MichaelvanLaar/n8n-nodes-defuddle.git"
  },
  "main": "index.js",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes --write",
    "lint": "eslint \"nodes/**/*.ts\"",
    "lintfix": "eslint \"nodes/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prepublishOnly": "npm run build && npm run lint && npm run test",
    "prepare": "husky"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [],
    "nodes": [
      "dist/nodes/Defuddle/Defuddle.node.js"
    ]
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/jsdom": "^27.0.0",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "eslint": "^9.38.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.4",
    "gulp": "^5.0.1",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "n8n-workflow": "^1.20.0",
    "prettier": "^3.6.2",
    "prettier-plugin-packagejson": "^2.5.19",
    "ts-jest": "^29.4.5",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.2"
  },
  "peerDependencies": {
    "n8n-workflow": "^1.20.0"
  },
  "dependencies": {
    "@types/turndown": "^5.0.5",
    "defuddle": "^0.6.6",
    "jsdom": "24.1.3",
    "turndown": "^7.2.1"
  }
}
